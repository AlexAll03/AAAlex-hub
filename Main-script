-- loading

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
  Name = "AAAlex hub with scripts",
  LoadingTitle = "Hi! Hub loading...",
  LoadingSubtitle = "by Ale89464",
  ConfigurationSaving = {
     Enabled = true,
     FolderName = nil, -- Create a custom folder for your hub/game
     FileName = "Big Hub"
  }, 
KeySystem = true, -- Set this to true to use our key system
KeySettings = {
   Title = "Stop!",
   Subtitle = "This script is private!",
   Note = "You can get it from owner, discord: question03",
   FileName = "Flu—Öus.dll " , -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
   SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
   GrabKeyFromSite = true, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
   Key = {"https://raw.githubusercontent.com/AlexAll03/AAAlex-hub/main/key"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
}
})

Rayfield:Notify({
   Title = "Loaded!",
   Content = " ",
   Duration = 6.5,
   Image = 4483362458
})

-- tabs

local sb = Window:CreateTab("Slap Battles!")

local Bedwars = Window:CreateTab("Bedwars!")

local universalscripts = Window:CreateTab("Universal Scripts")


--Slap Battles------------------------------------------------------------------------------------

local welcomesb = sb:CreateLabel("There are some scripts for slap battles and all gamemodes!")

--Slap Battles regular------------------------------

